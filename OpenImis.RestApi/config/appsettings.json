{
  "JwtIssuer": "http://openimis.org",
  "JwtAudience": "http://openimis.org",
  "JwtExpireDays": 5,
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "Language": {
    "Primary": "english,en,primary,0,1",
